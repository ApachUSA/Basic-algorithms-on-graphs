<Application x:Class="Graph_algorithms.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Graph_algorithms"
             xmlns:local2="clr-namespace:Graph_algorithms.Helpers"
             StartupUri="Views\MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/MainWindowStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <local2:CoordConvert x:Key="CoordConvert"/>
            <local2:StartConvert x:Key="StartConvert"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
